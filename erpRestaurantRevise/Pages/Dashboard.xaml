<Page x:Class="practice.Pages.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:practice.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="900"
      Title="Dashboard">

    <Grid Background="#FF051821" MaxHeight="900" MaxWidth="900">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0" Height="900" Width="900">
            <Grid Margin="42,99,621,576">
                <Button Content="Employee" HorizontalAlignment="Center" Margin="0,185,0,0" VerticalAlignment="Top" Foreground="White" FontSize="15" Background="{x:Null}" BorderBrush="{x:Null}"/>
            </Grid>
            <Grid Margin="336,99,328,576">
                <Button Content="Reservation" HorizontalAlignment="Center" Margin="0,185,0,0" VerticalAlignment="Top" Foreground="White" FontSize="15" BorderBrush="{x:Null}" Background="{x:Null}"/>
            </Grid>
            <Grid Margin="588,90,65,576">
                <Button Content="Attendance" HorizontalAlignment="Center" Margin="0,194,0,0" VerticalAlignment="Top" Foreground="White" FontSize="15" Background="{x:Null}" BorderBrush="{x:Null}"/>
            </Grid>
            <Grid Margin="62,352,569,314">
                <Button Content="Payroll Annual Salary" HorizontalAlignment="Left" Margin="59,194,0,0" VerticalAlignment="Top" Foreground="White" FontSize="15" Background="{x:Null}" BorderBrush="{x:Null}"/>
            </Grid>
            <Grid Margin="336,352,295,314">
                <Button x:Name="positionTableBtn" Content="Position Table" HorizontalAlignment="Left" Margin="59,194,0,0" VerticalAlignment="Top" FontSize="15" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Click="positionTableBtn_Click"/>
            </Grid>
        </Grid>
    </Grid>
</Page>
